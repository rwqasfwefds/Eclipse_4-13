package gettersetter06;

public class Employee {
	int number;
	String name;
	int money;
	
	Employee(int number, String name, int money){
		this.number = number;
		this.name = name;
		this.money = money;
	}

	public int getNumber() {
		return number;
	}

	public void setNumber(int number) {
		this.number = number;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getMoney() {
		return money;
	}

	public void setMoney(int money) {
		this.money = money;
	}
}
